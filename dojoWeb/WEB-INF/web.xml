<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

    <display-name>OSCARS Web Interface</display-name>
    <description>
      Web based interface to OSCARS Reservation System
    </description>

    <!-- Servlets in webapp -->

    <servlet>
        <servlet-name>WebSession</servlet-name>
        <servlet-class>net.es.oscars.dojoServlets.WebSession</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>WebSession</servlet-name>
        <url-pattern>/servlet/WebSession</url-pattern>
    </servlet-mapping>

    <!-- Security section -->

</web-app>
