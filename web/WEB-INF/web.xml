<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

    <display-name>OSCARS Web Interface</display-name>
    <description>
      Web based interface to OSCARS Reservation System
    </description>

    <!-- Servlets in webapp -->

    <servlet>
        <servlet-name>AuthenticateUser</servlet-name>
        <servlet-class>net.es.oscars.servlets.AuthenticateUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateReservationForm</servlet-name>
        <servlet-class>net.es.oscars.servlets.CreateReservationForm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateReservation</servlet-name>
        <servlet-class>net.es.oscars.servlets.CreateReservation</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>QueryReservation</servlet-name>
        <servlet-class>net.es.oscars.servlets.QueryReservation</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModifyReservation</servlet-name>
        <servlet-class>net.es.oscars.servlets.ModifyReservation</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CancelReservation</servlet-name>
        <servlet-class>net.es.oscars.servlets.CancelReservation</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListReservations</servlet-name>
        <servlet-class>net.es.oscars.servlets.ListReservations</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserQuery</servlet-name>
        <servlet-class>net.es.oscars.servlets.UserQuery</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserModify</servlet-name>
        <servlet-class>net.es.oscars.servlets.UserModify</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserAddForm</servlet-name>
        <servlet-class>net.es.oscars.servlets.UserAddForm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserAdd</servlet-name>
        <servlet-class>net.es.oscars.servlets.UserAdd</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserRemove</servlet-name>
        <servlet-class>net.es.oscars.servlets.UserRemove</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserList</servlet-name>
        <servlet-class>net.es.oscars.servlets.UserList</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserLogout</servlet-name>
        <servlet-class>net.es.oscars.servlets.UserLogout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AAA</servlet-name>
        <servlet-class>net.es.oscars.servlets.AAA</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AuthenticateUser</servlet-name>
        <url-pattern>/servlet/AuthenticateUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateReservationForm</servlet-name>
        <url-pattern>/servlet/CreateReservationForm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateReservation</servlet-name>
        <url-pattern>/servlet/CreateReservation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>QueryReservation</servlet-name>
        <url-pattern>/servlet/QueryReservation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ModifyReservation</servlet-name>
        <url-pattern>/servlet/ModifyReservation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CancelReservation</servlet-name>
        <url-pattern>/servlet/CancelReservation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListReservations</servlet-name>
        <url-pattern>/servlet/ListReservations</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserQuery</servlet-name>
        <url-pattern>/servlet/UserQuery</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserModify</servlet-name>
        <url-pattern>/servlet/UserModify</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserAddForm</servlet-name>
        <url-pattern>/servlet/UserAddForm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserAdd</servlet-name>
        <url-pattern>/servlet/UserAdd</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserRemove</servlet-name>
        <url-pattern>/servlet/UserRemove</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserList</servlet-name>
        <url-pattern>/servlet/UserList</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserLogout</servlet-name>
        <url-pattern>/servlet/UserLogout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AAA</servlet-name>
        <url-pattern>/servlet/AAA</url-pattern>
    </servlet-mapping>

</web-app>
