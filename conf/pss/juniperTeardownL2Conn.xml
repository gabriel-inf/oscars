<junoscript version="1.0" release="7.3">
<rpc><lock-configuration /></rpc>
<rpc>
    <load-configuration>
        <configuration>
            <interfaces>
                <interface>
                    <name><user-var>interface_a</user-var></name>
                    <unit delete="delete">
                        <name><user-var>local-vlan-id</user-var></name>
                    </unit>
                </interface>
                <interface>
                    <name><user-var>interface_b</user-var></name>
                    <unit delete="delete">
                        <name><user-var>remote-vlan-id</user-var></name>
                    </unit>
                </interface>
            </interfaces>
            <protocols>
                <connections>
                    <interface-switch delete="delete">
                        <name><user-var>resv-id</user-var></name>
                    </interface-switch>
                </connections>
            </protocols>
            <firewall>
                <policer delete="delete">
                    <name><user-var>resv-id</user-var></name>
                </policer>
                <family>
                    <ccc>
                        <filter delete="delete">
                            <name><user-var>resv-id</user-var></name>
                        </filter>
                    </ccc>
                </family>
            </firewall>
        </configuration>
    </load-configuration>
</rpc>
<rpc><commit-configuration /></rpc>
<rpc><unlock-configuration /></rpc>
</junoscript>
