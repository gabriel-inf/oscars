<configuration>
    <protocols>
        <mpls>
            <label-switched-path>
                <name>user_var_name_user_var</name>
                <from>user_var_lsp_from_user_var</from>
                <to>user_var_lsp_to_user_var</to>
                <metric>65535</metric>
                <bandwidth>user_var_bandwidth_user_var</bandwidth>
                <class-of-service>user_var_lsp_class-of-service_user_var</class-of-service>
                <no-cspf/>
                <setup-priority>user_var_lsp_setup-priority_user_var</setup-priority>
                <reservation-priority>user_var_lsp_reservation-priority_user_var</reservation-priority>
                <description>user_var_lsp_description_user_var</description>
            </label-switched-path>
        </mpls>
    </protocols>
    <firewall>
        <policer>
            <name>user_var_name_user_var</name>
            <filter-specific/>
            <if-exceeding>
                <bandwidth-limit>user_var_bandwidth_user_var</bandwidth-limit>
                <burst-size-limit>user_var_policer_burst-size-limit_user_var</burst-size-limit>
            </if-exceeding>
            <then>
                <discard/>
            </then>
        </policer>
        <family>
            <inet>
                <filter>
                    <name>user_var_external_interface_filter_user_var</name>
                    <term>
                    <name>user_var_name_user_var</name>
                    <from>
                        <source-address>user_var_source-address_user_var</source-address>
                        <destination-address>user_var_destination-address_user_var</destination-address>
                        <dscp>user_var_dscp_user_var</dscp>
                        <protocol>user_var_protocol_user_var</protocol>
                        <source-port>user_var_source-port_user_var</source-port>
                        <destination-port>user_var_destination-port_user_var</destination-port>
                    </from>
                    <then>
                        <policer>user_var_name_user_var</policer>
                        <count>user_var_name_user_var</count>
                        <forwarding-class>expedited-forwarding</forwarding-class>
                        <routing-instance>user_var_name_user_var</routing-instance>
                    </then>
                    </term>
                    <term insert="after" name="user_var_firewall_filter_marker_user_var">
                        <name>user_var_name_user_var</name>
                    </term>
                </filter>
            </inet>
        </family>
    </firewall>
    <routing-instances>
        <instance>
            <name>user_var_name_user_var</name>
            <instance-type>forwarding</instance-type>
            <routing-options>
                <static>
                    <route>
                        <destination>user_var_destination-address_user_var</destination>
                        <lsp-next-hop>
                            <name>user_var_name_user_var</name>
                        </lsp-next-hop>
                    </route>
                </static>
            </routing-options>
        </instance>
    </routing-instances>
</configuration>
