<junoscript version="1.0" release="7.3">
<rpc><lock-configuration /></rpc>
<rpc>
    <load-configuration>
        <configuration>
            <protocols>
                <mpls>
                    <label-switched-path delete="delete">
                        <name><user-var>resv-id</user-var></name>
                    </label-switched-path>
                    <path delete="delete">
                        <name><user-var>resv-id</user-var></name>
                    </path>
                </mpls>
            </protocols>
            <firewall>
                <policer delete="delete">
                    <name><user-var>resv-id</user-var></name>
                </policer>
                <family>
                    <inet>
                        <filter>
                            <name><user-var>internal_interface_filter</user-var></name>
                            <term delete="delete">
                                <name><user-var>resv-id</user-var></name>
                            </term>
                        </filter>
                        <filter>
                            <name><user-var>external_interface_filter</user-var></name>
                            <term delete="delete">
                                <name><user-var>resv-id</user-var></name>
                            </term>
                        </filter>
                    </inet>
                    <any>
                        <filter delete="delete">
                            <name><user-var>resv-id</user-var></name>
                        </filter>
                    </any>
                </family>
            </firewall>
            <routing-instances>
                <instance delete="delete">
                    <name><user-var>resv-id</user-var></name>
                </instance>
            </routing-instances>
        </configuration>
    </load-configuration>
</rpc>
<rpc><commit-configuration /></rpc>
<rpc><unlock-configuration /></rpc>
</junoscript>
