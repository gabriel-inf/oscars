<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

    <display-name>ION</display-name>
    <description>
      Web ION interface 
    </description>
	<servlet>
        	<servlet-name>AuthenticateUser</servlet-name>
        	<servlet-class>edu.internet2.ion.ionui.servlets.AuthenticateUser</servlet-class>
        	<load-on-startup>1</load-on-startup>
	</servlet>

	<servlet-mapping>
        	<servlet-name>AuthenticateUser</servlet-name>
        	<url-pattern>/servlet/AuthenticateUser</url-pattern>
    	</servlet-mapping>
</web-app>
