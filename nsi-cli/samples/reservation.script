
resv load --name myresv
resv set --st "10 sec" --et "30 min"
resv set --sv 1782 --dv 1782


nsi reserve
nsi wait --type "RESERVE_CONFIRMED"
nsi commit
nsi wait --type "COMMIT_CONFIRMED"
nsi provision
nsi wait --type "PROVISION_CONFIRMED"

sleep --sec 120

// modify
resv set --b 2
nsi reserve
nsi wait --type "RESERVE_CONFIRMED"

sleep --sec 20
nsi terminate