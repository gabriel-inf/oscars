resv load --name myresv
resv set --st "30 sec" --et "30 min"

nsi reserve
nsi wait --type "RESERVE_CONFIRMED"
nsi commit
nsi wait --type "COMMIT_CONFIRMED"
nsi provision
nsi wait --type "PROVISION_CONFIRMED"

sleep --sec 20

// modify
resv set --b 2
nsi reserve
nsi wait --type "RESERVE_CONFIRMED"

sleep --sec 20
nsi terminate