<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Adapted from mapping created by the Middlegen Hibernate plugin 2.1.

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class name="net.es.oscars.bss.Reservation" table="reservations">
    <meta attribute="class-description" inherit="false">
       @hibernate.class table="reservations"
    </meta>

    <id name="id">
        <meta attribute="field-description">
           @hibernate.id generator-class="increment"
        </meta>
        <generator class="increment" />
    </id>

    <property name="startTime" not-null="true">
        <meta attribute="field-description">
           @hibernate.property not-null="true"
        </meta>    
    </property>

    <property name="endTime" not-null="true">
        <meta attribute="field-description">
           @hibernate.property not-null="true"
        </meta>    
    </property>

    <property name="createdTime" not-null="true">
        <meta attribute="field-description">
           @hibernate.property not-null="true"
        </meta>    
    </property>

    <property name="bandwidth" not-null="true">
        <meta attribute="field-description">
           @hibernate.property not-null="true"
        </meta>    
    </property>

    <property name="burstLimit" not-null="true">
        <meta attribute="field-description">
           @hibernate.property not-null="true"
        </meta>    
    </property>

    <property name="login" not-null="true">
        <meta attribute="field-description">
           @hibernate.property not-null="true"
        </meta>    
    </property>

    <property name="status" not-null="true">
        <meta attribute="field-description">
           @hibernate.property not-null="true"
        </meta>    
    </property>

    <property name="lspClass" not-null="true">
        <meta attribute="field-description">
           @hibernate.property not-null="true"
        </meta>    
    </property>

    <property name="srcHost" not-null="true">
        <meta attribute="field-description">
           @hibernate.property not-null="true"
        </meta>    
    </property>

    <property name="destHost" not-null="true">
        <meta attribute="field-description">
           @hibernate.property not-null="true"
        </meta>    
    </property>

    <property name="srcPort" />

    <property name="destPort" />

    <property name="dscp" />

    <property name="protocol" />

    <property name="description" />

    <!-- Associations -->

    <many-to-one name="path" column="pathId"
	    class="net.es.oscars.bss.topology.Path" not-null="true" />

</class>
</hibernate-mapping>
