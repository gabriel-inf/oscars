<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>

<class name="net.es.oscars.bss.topology.L2SwitchingCapabilityData" table="l2SwitchingCapabilityData">
    <meta attribute="class-description" inherit="false">
       @hibernate.class table="l2SwitchingCapabilityData"
    </meta>

    <id name="id">
        <meta attribute="field-description">
           @hibernate.id generator-class="identity"
        </meta>
        <generator class="identity" />
    </id>

    <property name="vlanRangeAvailability" not-null="true">
        <meta attribute="field-description">
           @hibernate.property not-null="true"
        </meta>    
    </property>

    <property name="interfaceMTU" not-null="true">
        <meta attribute="field-description">
           @hibernate.property not-null="true"
        </meta>    
    </property>

    <!-- Associations -->

    <many-to-one name="link" column="linkId"
	    class="net.es.oscars.bss.topology.Link" 
	    unique="true" />

</class>
</hibernate-mapping>
