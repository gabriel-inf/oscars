<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
<!--
    Adapted from mapping created by the Middlegen Hibernate plugin 2.1.

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class name="net.es.oscars.bss.topology.Link" table="links">
    <meta attribute="class-description" inherit="false">
       @hibernate.class table="links"
    </meta>

    <cache usage="read-write" />

    <id name="id">
        <meta attribute="field-description">
           @hibernate.id generator-class="identity"
        </meta>
        <generator class="identity" />
    </id>

    <property name="valid" not-null="true">
        <meta attribute="field-description">
           @hibernate.property not-null="true"
        </meta>
        <column sql-type="BOOLEAN" name="valid" />
    </property>

    <property name="snmpIndex" not-null="true">
        <meta attribute="field-description">
           @hibernate.property not-null="true"
        </meta>
    </property>

    <property name="topologyIdent" not-null="true">
        <meta attribute="field-description">
           @hibernate.property not-null="true"
        </meta>
    </property>

    <property name="trafficEngineeringMetric" />

    <property name="capacity" />

    <property name="maximumReservableCapacity" />

    <property name="minimumReservableCapacity" />

    <property name="granularity" />

    <property name="unreservedCapacity" />

    <property name="alias" />


    <!-- Associations -->

    <many-to-one name="port" column="portId"
        class="net.es.oscars.bss.topology.Port" not-null="true" />

    <many-to-one name="remoteLink" column="remoteLinkId"
        class="net.es.oscars.bss.topology.Link" />

    <set name="ipaddrs" inverse="true" cascade="all, delete-orphan">
        <key column="linkId" />
        <one-to-many class="net.es.oscars.bss.topology.Ipaddr" />
    </set>

    <one-to-one name="l2SwitchingCapabilityData"
        class="net.es.oscars.bss.topology.L2SwitchingCapabilityData"
        property-ref="link" />


</class>
</hibernate-mapping>
