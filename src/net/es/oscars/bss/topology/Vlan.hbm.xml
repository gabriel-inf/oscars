<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>

<class name="net.es.oscars.bss.topology.Vlan" table="vlans">
    <meta attribute="class-description" inherit="false">
       @hibernate.class table="vlans"
    </meta>

    <id name="id">
        <meta attribute="field-description">
           @hibernate.id generator-class="identity"
        </meta>
        <generator class="identity" />
    </id>

    <property name="vlanTag" not-null="true">
        <meta attribute="field-description">
           @hibernate.property not-null="true"
        </meta>    
    </property>

    <!-- Associations -->

    <many-to-one name="port" column="portId"
	    class="net.es.oscars.bss.topology.Port" not-null="true" />

</class>
</hibernate-mapping>
