<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
<!--
    Adapted from mapping created by the Middlegen Hibernate plugin 2.1.

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class name="net.es.oscars.bss.topology.Ipaddr" table="ipaddrs">
    <meta attribute="class-description" inherit="false">
       @hibernate.class table="ipaddrs"
    </meta>

    <cache usage="read-write" />

    <id name="id">
        <meta attribute="field-description">
           @hibernate.id generator-class="identity"
        </meta>
        <generator class="identity" />
    </id>

    <property name="valid" not-null="true">
        <meta attribute="field-description">
           @hibernate.property not-null="true"
        </meta>
        <column sql-type="BOOLEAN" name="valid" />
    </property>

    <property name="IP" not-null="true">
        <meta attribute="field-description">
           @hibernate.property not-null="true"
        </meta>
    </property>

    <!-- Associations -->

    <many-to-one name="link" column="linkId"
        class="net.es.oscars.bss.topology.Link" not-null="true" />

</class>
</hibernate-mapping>
