<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<class name="net.es.oscars.bss.History" table="history">
    <meta attribute="class-description" inherit="false">
       @hibernate.class table="history"
    </meta>

    <id name="id">
        <meta attribute="field-description">
           @hibernate.id generator-class="identity"
        </meta>
        <generator class="identity" />
    </id>
    

    <property name="traceId" not-null="true">
        <meta attribute="field-description">
           @hibernate.property not-null="true"
        </meta>    
    </property>
    
    <property name="description" not-null="true">
        <meta attribute="field-description">
           @hibernate.property not-null="true"
        </meta>    
    </property>
    
    <property name="operationType" not-null="true">
        <meta attribute="field-description">
           @hibernate.property not-null="true"
        </meta>    
    </property>
    

    <property name="operationTime" not-null="true">
        <meta attribute="field-description">
           @hibernate.property not-null="true"
        </meta>    
    </property>

    <property name="result" not-null="true">
        <meta attribute="field-description">
           @hibernate.property not-null="true"
        </meta>    
    </property>
    
    <property name="receivedFrom" not-null="true">
        <meta attribute="field-description">
           @hibernate.property not-null="true"
        </meta>    
    </property>
    
    <property name="forwardedTo" not-null="true">
        <meta attribute="field-description">
           @hibernate.property not-null="true"
        </meta>    
    </property>


    <!-- Associations -->

    <many-to-one name="reservation" column="reservationId"
	    class="net.es.oscars.bss.Reservation" cascade="all"
	    not-null="true" />
	    

</class>
</hibernate-mapping>
