<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
<!--
    Adapted from mapping created by the Middlegen Hibernate plugin 2.1.

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class name="net.es.oscars.aaa.Authorization" table="authorizations">
    <meta attribute="class-description" inherit="false">
       @hibernate.class table="Authorizations"
    </meta>

    <id name="id">
        <meta attribute="field-description">
           @hibernate.id generator-class="identity"
        </meta>
        <generator class="identity" />
    </id>

    <property name="context" />

    <property name="updateTime" />

    <!-- Associations -->
    <many-to-one name="attribute" column="attrId"
        class="net.es.oscars.aaa.Attribute"  />

    <many-to-one name="constraint" column="constraintId"
        class="net.es.oscars.aaa.Constraint" />

    <many-to-one name="resource" column="resourceId"
        class="net.es.oscars.aaa.Resource" />

    <many-to-one name="permission" column="permissionId"
        class="net.es.oscars.aaa.Permission"  />

    <property name="constraintValue"/>

    <!-- Associations -->

</class>
</hibernate-mapping>
