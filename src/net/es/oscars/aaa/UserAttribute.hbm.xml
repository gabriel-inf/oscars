<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
<!--
    Adapted from mapping created by the Middlegen Hibernate plugin 2.1.

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class name="net.es.oscars.aaa.UserAttribute" table="userAttributes">
    <meta attribute="class-description" inherit="false">
       @hibernate.class table="userAttributes"
    </meta>

    <id name="id">
        <meta attribute="field-description">
           @hibernate.id generator-class="identity"
        </meta>
        <generator class="identity" />
    </id>

    <!-- Associations -->
    <many-to-one name="user" column="userId"
        class="net.es.oscars.aaa.User" />

    <many-to-one name="attribute" column="attributeId"
        class="net.es.oscars.aaa.Attribute" />


</class>
</hibernate-mapping>
