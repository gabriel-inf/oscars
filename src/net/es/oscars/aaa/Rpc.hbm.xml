<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>

<class name="net.es.oscars.aaa.Rpc" table="rpcs">
    <meta attribute="class-description" inherit="false">
       @hibernate.class table="rpcs"
    </meta>

    <id name="id">
        <meta attribute="field-description">
           @hibernate.id generator-class="identity"
        </meta>
        <generator class="identity" />
    </id>

    <!-- Associations -->
    <many-to-one name="constraint" column="constraintId"
        unique="true" class="net.es.oscars.aaa.Constraint" />

    <many-to-one name="resource" column="resourceId"
        unique="true" class="net.es.oscars.aaa.Resource" />

    <many-to-one name="permission" column="permissionId"
        unique="true" class="net.es.oscars.aaa.Permission" />

</class>
</hibernate-mapping>
