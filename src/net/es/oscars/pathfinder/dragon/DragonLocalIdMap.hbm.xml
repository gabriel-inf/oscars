<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Adapted from mapping created by the Middlegen Hibernate plugin 2.1.

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class name="net.es.oscars.pathfinder.dragon.DragonLocalIdMap" table="dragonLocalIdMap">
    <meta attribute="class-description" inherit="false">
       @hibernate.class table="dragonLocalIdMap"
    </meta>

    <id name="id">
        <meta attribute="field-description">
           @hibernate.id generator-class="increment"
        </meta>
        <generator class="increment" />
    </id>

    <property name="ip" not-null="true">
        <meta attribute="field-description">
           @hibernate.property not-null="true"
        </meta>    
    </property>
    
    <property name="number" not-null="true">
        <meta attribute="field-description">
           @hibernate.property not-null="true"
        </meta>    
    </property>
    
    <property name="type" not-null="true">
        <meta attribute="field-description">
           @hibernate.property not-null="true"
        </meta>    
    </property>


    <!-- Associations -->

    <many-to-one name="vlsrIp" column="vlsrIpId"
	    class="net.es.oscars.bss.topology.Ipaddr" not-null="true" />

</class>
</hibernate-mapping>
