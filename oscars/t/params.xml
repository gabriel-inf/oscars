	<createReservation>
            <origTimeZone>-07:00</origTimeZone>
            <bandwidth>25</bandwidth>
            <protocol>udp</protocol>
            <description>Dummy testForward test.</description>
	    <srcHost>nettrash3.es.net</srcHost>
	    <destHost>nms5-ipls.abilene.ucaid.edu</destHost>
	    <egressRouterIP>chi-cr1.es.net</egressRouterIP>
	</createReservation>
	<payloadSender>oscars.es.net</payloadSender>
	<userLogin>
	    <userName>testaccount</userName>
