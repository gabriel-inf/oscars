<persistence xmlns="http://java.sun.com/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
             version="2.0">
    <persistence-unit name="net.es.oscars.nsibridge">
        <provider>org.hibernate.ejb.HibernatePersistence</provider>
        <class>net.es.oscars.nsibridge.beans.db.ConnectionRecord</class>
        <class>net.es.oscars.nsibridge.beans.db.ResvRecord</class>
        <class>net.es.oscars.nsibridge.beans.db.DataplaneStatusRecord</class>
        <class>net.es.oscars.nsibridge.beans.db.OscarsStatusRecord</class>
        <class>net.es.oscars.nsibridge.beans.db.ExceptionRecord</class>
        <class>net.es.oscars.nsibridge.beans.db.NotificationRecord</class>
        <class>net.es.oscars.nsibridge.beans.db.OscarsInfoRecord</class>
        <properties>
            <property name="hibernate.ejb.cfgfile" value="hibernate.xml"/>
        </properties>
    </persistence-unit>

</persistence>